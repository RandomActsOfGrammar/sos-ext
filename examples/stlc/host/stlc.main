Module stlc:host

Function main : <Args : [string]> -> int {
   Read Args[0] to Contents
   ParseSuccess := Parse term_c as Tm from Contents
   If ParseSuccess
   Then
     Success := Derive typeOf nil Tm Ty
     If Success
       Then Print "Type :"
            Print Ty
            Return 0
       Else Print "No type"
            Return 1
       End
   Else
     Print "Error parsing file"
     Print Args[0]
     Print "\n"
   End
}
